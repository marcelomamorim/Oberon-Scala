MODULE bee1079Average;

VAR
    a : ARRAY 9 OF REAL;
    ans : ARRAY 3 OF REAL;
    aux : REAL;
    i: INTEGER;

BEGIN
    i := 0;
    
    a[0] := 6.5;
    a[1] := 4.3;
    a[2] := 6.2;
    a[3] := 5.1;
    a[4] := 4.2;
    a[5] := 8.1;
    a[6] := 8.0;
    a[7] := 9.0;
    a[8] := 10.0;

    

    REPEAT
        aux := a[3 * i] * 2 + a[3 * i + 1] * 3+ a[3 * i + 2] * 5;
        aux := aux / 10;
        ans[i] := (aux);
        i := i + 1
           
    UNTIL (i = 3)
    

END

END bee1079Average.compile